{
  "name": "${{ values.name }}",
  "version": "1.0.0",
  "description": "${{ values.description }}",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "nodejs",
    "express",
    "api",
    "rest",
    "microservice"
  ],
  "author": "${{ values.owner }}",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    {% if values.databaseType == "postgresql" %}
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    {% elif values.databaseType == "mysql" %}
    "mysql2": "^3.6.5",
    {% elif values.databaseType == "mongodb" %}
    "mongoose": "^8.0.3",
    {% elif values.databaseType == "redis" %}
    "redis": "^4.6.11",
    {% endif %}
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=${{ values.nodeVersion }}"
  }
}
