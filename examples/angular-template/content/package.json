{
  "name": "${{ values.name }}",
  "version": "1.0.0",
  "description": "${{ values.description }}",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port ${{ values.port }}",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^${{ values.angularVersion }}.0.0",
    "@angular/common": "^${{ values.angularVersion }}.0.0",
    "@angular/compiler": "^${{ values.angularVersion }}.0.0",
    "@angular/core": "^${{ values.angularVersion }}.0.0",
    "@angular/forms": "^${{ values.angularVersion }}.0.0",
    "@angular/platform-browser": "^${{ values.angularVersion }}.0.0",
    "@angular/platform-browser-dynamic": "^${{ values.angularVersion }}.0.0",
    "@angular/router": "^${{ values.angularVersion }}.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^${{ values.angularVersion }}.0.0",
    "@angular/cli": "^${{ values.angularVersion }}.0.0",
    "@angular/compiler-cli": "^${{ values.angularVersion }}.0.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.11.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.0"
  }
}
